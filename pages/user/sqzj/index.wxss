/* pages/user/sqzj/index.wxss */
.sq_content {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 30rpx 50rpx;
  box-shadow: #fdccd1 0rpx 0rpx 20rpx 2rpx;
  border-radius: 12rpx;
}
.sq_title {
  color: #000;
  font-size: 38rpx;
  margin: 60rpx 0 26rpx 
}
.form_content {
  display: flex;
  flex-direction: column;
  width: 100%;
}
.form_content .form_item_grp {
  display: flex;
  flex-direction: column;
  padding: 0 30rpx
}
.form_content .form_item_grp input{
  border-bottom: solid 1rpx #eee;
  height: 90rpx;
  width: 100%;
  padding: 0 10rpx;
  font-size: 28rpx;
  color: #181818
}
.btn_grp {
  margin: 110rpx 50rpx 30rpx 50rpx;
}
.btn_grp button {
  background: #f9354b;
  font-size: 34rpx;
  color: #fff;
}
.sq_sm {
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-size: 22rpx;
  margin-bottom: 160rpx
}
.sq_sm text{
  color: #bbb
}
.sq_sm  navigator {
  color: #28bff1
}
