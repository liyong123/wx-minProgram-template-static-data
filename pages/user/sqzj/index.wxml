      <form class='form_content' bindsubmit='formSubmit'>
        <view class='form_item_grp'>
          <input type='text' name="telephone" placeholder='您的真实姓名' placeholder-style='color: #999' />
          <view class='upload_grp'>
             <view>
               <text>请上传已获得的证书</text>
               
             </view>
          </view>
        </view>


      </form>
   </view>
   <view>
